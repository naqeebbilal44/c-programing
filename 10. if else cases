#include <stdio.h>

int main() {
    int marks;

    // Input marks from the user
    printf("Enter your marks (out of 100): ");
    scanf("%d", &marks);

    // Check grade using if-else
    if (marks >= 90) {
        printf("Grade: A\n");
    }
    else if (marks >= 80) {
        printf("Grade: B\n");
    }
    else if (marks >= 70) {
        printf("Grade: C\n");
    }
    else if (marks >= 60) {
        printf("Grade: D\n");
    }
    else if (marks >= 50) {
        printf("Grade: E\n");
    }
    else {
        printf("Grade: F (Fail)\n");
    }

    return 0;
}
